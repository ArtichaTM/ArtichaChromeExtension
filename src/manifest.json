{
	"manifest_version": 3,
	"name": "ArtichaSiteExtension",
	"version": "1.2.1",
	"description": "Useful utilities provided by Articha site",
	"homepage_url": "https://articha.tplinkdns.com/chrome-extension/",
	"update_url": "https://articha.tplinkdns.com/chrome-extension/updates.xml",

	"icons": {
		"16": "images/icon/16.png",
		"32": "images/icon/32.png",
		"128": "images/icon/128.png"
	},
	"content_scripts": [{
		"js": ["scripts/kish_sender.js"],
		"matches": [
			"https://kispython.ru/files/*"
		]
	}]
}
